#!/bin/bash

if [ -e "Makefile" ]; then
	echo "removing Makefile"
	rm Makefile
fi

echo "creating Makefile"
qmake dsipcom.pro
if [ $? = 0 ]; then
	echo "done"
fi

#make clean
make all 2> compile.log
if [ $? = 0 ]; then
	 echo "done. dSipCom ready"
fi

if [ -s "compile.log" ]; then
	less compile.log
fi

